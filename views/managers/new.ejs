<% layout('layouts/boilerplate') %>
<form action="/managers" method="POST">
  <div>
    <label for="title">Title</label>
    <input type="text" id="title" name="manager[name]">
  </div>
  <div>
    <label for="description">Description</label>
    <input type="text" id="description" name="manager[description]">
  </div>
  <div>
    <label for="wins">Wins</label>
    <input type="text" id="wins" name="manager[wins]">
  </div>
  <div>
    <label for="losses">Losses</label>
    <input type="text" id="losses" name="manager[losses]">
  </div>
  <button>Add Manager</button>
  <div>
    <a href="/managers">Back to All Managers -></a>
  </div>
</form>